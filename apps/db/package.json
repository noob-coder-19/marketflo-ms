{
  "name": "database",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "seed:db": "tsup --clean --onSuccess \"node dist/seed-db.js\""
  },
  "dependencies": {
    "@repo/logger": "workspace:*",
    "@repo/models": "workspace:*",
    "dotenv": "^16.4.7",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.10",
    "@types/redis": "^4.0.11",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  }
}
